pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo "WOrkspace is:: ${WORKSPACE}"
                sh 'echo "Hello World"'
                sh '''
                    echo "Multiline shell steps works too..ddd"
                    ls -lah
                '''

                echo "sh path: ${WORKSPACE}/TempSH/temp.sh"
                sh '/${WORKSPACE}/TempSH/temp.sh'
            }
        }
    }
}
